{
  "manifest_version": 2,

  "name": "Recently Closed",
  "description": "Keep track of recently closed tabs",
  "version": "0.1",
  "permissions": [
    "tabs",
    "activeTab",
	"storage",
	"http://*/",
	"https://*/",
    "chrome://favicon/"
  ],
  "background": {
    "page": "dist/html/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "dist/img/structify_medium.png"
  },
  "chrome_url_overrides" : {
	  "newtab": "dist/html/interface.html"
  },
  "icons": {
    "16": "dist/img/structify_small.png",
    "48": "dist/img/structify_medium.png",
    "128": "dist/img/structify.png"
  }

}
